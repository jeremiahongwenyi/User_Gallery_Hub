{
    "version": 2,
    "name": "user-gallery-hub",
    "framework": "angular",
    "outputDirectory": "dist/user-gallery-hub/browser/browser",
    "routes": [
      { "handle": "filesystem" },
      { 
        "src": "/(assets/.+)",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }